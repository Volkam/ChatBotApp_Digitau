<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape  android:shape="rectangle">

            <solid android:color="@color/SombraLaranja" />
            <corners android:radius="6dp" />

        </shape>
    </item>
    <item android:bottom="4dp">
        <shape  android:shape="rectangle">
            <corners
                android:radius="6dp"
                />
            <solid
                android:color="@color/LaranjaItau"
                />
            <padding
                android:left="40dp"
                android:top="10dp"
                android:right="40dp"
                android:bottom="10dp"
                />
        </shape>
    </item>

</layer-list>
